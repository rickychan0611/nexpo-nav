{
  "expo": {
    "name": "Tin Tin Food Wholesale",
    "slug": "tin-tin-food",
    "privacy": "public",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "ca.tintin.tintinfood",
      "buildNumber": "1.0.0",
      "requireFullScreen": true,
      "supportsTablet": true
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "package": "ca.tintin.tintinfood",
      "versionCode": 3,
      "permissions": [
        "CAMERA", "RECORD_AUDIO", "READ_PHONE_STATE", "READ_CONTACTS"
      ]
    },
    "orientation": "portrait",
    "updates": {
      "fallbackToCacheTimeout": 0
    }
  }
}